  </ItemGroup>

  <ItemGroup>
    <Compile Update="Strategies\Class Generation\AbstractClassGeneration.feature.cs">
      <DependentUpon>AbstractClassGeneration.feature</DependentUpon>
    </Compile>
    <Compile Update="Strategies\Class Generation\StaticClassGeneration.feature.cs">
      <DependentUpon>StaticClassGeneration.feature</DependentUpon>
    </Compile>
    <Compile Update="Strategies\Class Generation\StandardClassGeneration.feature.cs">
      <DependentUpon>StandardClassGeneration.feature</DependentUpon>
    </Compile>
    <Compile Update="Strategies\Operator Generation\CanCallOperatorGeneration.feature.cs">
      <DependentUpon>CanCallOperatorGeneration.feature</DependentUpon>
    </Compile>
    <Compile Update="Strategies\Property Generator\ReadOnlyPropertyGeneration.feature.cs">
