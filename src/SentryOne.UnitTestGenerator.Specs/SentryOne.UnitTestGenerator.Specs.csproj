<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>

    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Class Generation\AbstractClassGeneration - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Class Generation\ComparableStrategy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Class Generation\StandardClassGeneration - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Indexer Generation\ReadOnlyIndexerGeneration - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Indexer Generation\WriteOnlyIndexerGeneration - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Operator Generation\NullParameterCheckConstructorGenerator.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Operator Generation\NullParameterCheckOperatorGenerator - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Property Generator\MultiConstructorInitialisedPropertyGeneration - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Property Generator\ReadOnlyIndexerGeneration.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Property Generator\ReadOnlyPropertyGeneration - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Property Generator\ReadWritePropertyGeneration - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Property Generator\WriteOnlyPropertyGeneration - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Strategies\Regeneration\StandardClassGeneration.feature.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="nunit" Version="3.11.0" />
    <PackageReference Include="NUnit3TestAdapter" Version="3.15.1" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="15.9.0" />
    <PackageReference Include="SpecFlow.NUnit" Version="3.0.225" />
    <PackageReference Include="SpecFlow.Tools.MsBuild.Generation" Version="3.0.225" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SentryOne.UnitTestGenerator.Core\SentryOne.UnitTestGenerator.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Strategies\InterfaceGeneration\ComparableStrategy.feature">
      <Generator>SpecFlowSingleFileGenerator</Generator>
      <LastGenOutput>ComparableStrategy.feature.cs</LastGenOutput>
    </None>
  </ItemGroup>

  <ItemGroup>
    <SpecFlowFeatureFiles Update="Strategies\Class Generation\AbstractClassGeneration.feature">
